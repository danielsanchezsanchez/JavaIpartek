<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	      version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>filtros</filter-name>
  	<filter-class>com.tienda.programa.filtros.filtros</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>filtros</filter-name>
  	<url-pattern>/filtros</url-pattern>
  </filter-mapping>
  <listener>
  	<listener-class>com.tienda.programa.listeners.listener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>ControladorIndex</servlet-name>
  	<servlet-class>com.tienda.programa.servlets.ControladorIndex</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>ControladorUsuarios</display-name>
  	<servlet-name>ControladorUsuarios</servlet-name>
  	<servlet-class>com.tienda.programa.servlets.ControladorUsuarios</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>ControladorAdministrador</display-name>
  	<servlet-name>ControladorAdministrador</servlet-name>
  	<servlet-class>com.tienda.programa.servlets.ControladorAdministrador</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControladorIndex</servlet-name>
  	<url-pattern>/controladorIndex</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControladorUsuarios</servlet-name>
  	<url-pattern>/usuarios/controladorUsuarios</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControladorAdministrador</servlet-name>
  	<url-pattern>/admin/controladorAdministrador</url-pattern>
  </servlet-mapping>
</web-app>
